{
  "name": "algolia-rules-checker",
  "version": "1.0.0",
  "description": "Simple tool to check which rules are applied to a specified query 🔍",
  "license": "MIT",
  "main": "index.html",
  "scripts": {
    "start": "parcel index.html --open && cp -f _headers dist/_headers",
    "build": "parcel build index.html && cp -f _headers dist/_headers"
  },
  "dependencies": {
    "algoliasearch": "4.13.0",
    "dotenv": "^16.0.0",
    "parcel-bundler": "^1.6.1"
  },
  "devDependencies": {
    "@babel/core": "7.2.0",
    "typescript": "4.4.4"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "keywords": [
    "algolia",
    "rules"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
